% A3-11a.488   
% test statement checks
{
    var p, q : boolean
    var i : integer
    while p and q do
        while ( p ? false : q ) do
            while i not = 3 do
                 while i = 3 do {
                      exit 2 when p or q
                      exit 1
		      if q then
                         exit 4 
                      else
                         exit 3 when i < 7
                 }
}
