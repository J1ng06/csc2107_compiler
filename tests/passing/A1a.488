{
    % declarations
    var num1 : integer
    var num2, num3, result : integer
    var bool1, bool2 : boolean  
        
    num1 := 10
    num2 := 20
    bool1 := true
    bool2 := false
        
    % arithmetic operators
    result := num1 + 1
    result := num1 - (-num2)
    result := num1 * 3
    result := num1 / num2

    % logical operators
    if (not bool2) then num3 := 3
    if (bool1 and (not bool2)) then num3 := 4
    if (bool1 or bool2) then num3 := 5
        
    % comparison operators
    if (bool1 = true) then num3 := 2
    if (num1 not= 11) then bool1 := false
    if (num1 < num2) then bool2 := true
    if (num1 <= num2) then bool2 := true
    if (num1 > num2) then bool2 := true
    if (num1 >= num2) then bool2 := true

    % conditional expression
    bool2 := (num1 < num2 ? true:false) 
}
